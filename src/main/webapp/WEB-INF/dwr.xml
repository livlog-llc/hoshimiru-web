<?xml version="1.0" encoding="UTF-8"?>
<dwr>
	<!-- without allow, DWR isn't allowed to do anything -->
	<allow>
		<create creator="new" javascript="StatusAjax" scope="application">
			<param name="class" value="jp.livlog.hoshimiru.dwr.StatusAjax" />
		</create>
		<convert converter="bean" match="jp.livlog.hoshimiru.helper.hoshimiru.StarrySkyData" />
	</allow>
		<allow>
		<create creator="new" javascript="MeteorStreamAjax" scope="application">
			<param name="class" value="jp.livlog.hoshimiru.dwr.MeteorStreamAjax" />
		</create>
	</allow>
		<allow>
		<create creator="new" javascript="AstroCalendarAjax" scope="application">
			<param name="class" value="jp.livlog.hoshimiru.dwr.AstroCalendarAjax" />
		</create>
	</allow>
	<allow>
		<create creator="new" javascript="StarAjax" scope="application">
			<param name="class" value="jp.livlog.hoshimiru.dwr.StarAjax" />
		</create>
		<convert converter="bean" match="jp.livlog.hoshimiru.helper.hoshimiru.ConstellationData" />
	</allow>
	<allow>
		<create creator="new" javascript="PlanetAjax" scope="application">
			<param name="class" value="jp.livlog.hoshimiru.dwr.PlanetAjax" />
		</create>
		<convert converter="bean" match="jp.livlog.hoshimiru.helper.hoshimiru.PlanetData" />
	</allow>
</dwr>